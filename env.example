# Frontend Environment Variables
# Copy this to .env or .env.production and set your values

# Backend API Base URL (optional - auto-detected in production)
# If not set, the frontend will try to auto-detect based on hostname
# Examples:
#   VITE_API_BASE_URL=http://167.172.90.182:7500
#   VITE_API_BASE_URL=http://yourdomain.com:7500
#   VITE_API_BASE_URL=https://api.yourdomain.com
VITE_API_BASE_URL=

# Gemini API Key (if using AI features)
GEMINI_API_KEY=

